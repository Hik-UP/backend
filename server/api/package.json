{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "main": "src/server/https.ts",
  "scripts": {
    "build": "npx env-cmd -f ./config/.env.production npx tsc",
    "deploy": "npx env-cmd -f ./config/.env.production node dist/server/https.js",
    "dev": "npx env-cmd -f ./config/.env.development concurrently \"npx tsc --watch\" \"nodemon --trace-deprecation -q dist/server/https.js\"",
    "test": "npx env-cmd -f ./config/.env.test npx jest",
    "prettier": "npx prettier 'src/**/*.ts' --write"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
